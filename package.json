{
  "name": "pretty-jscpd",
  "version": "0.1.0",
  "description": "Understand your duplicated code distribution better by enhacing JSCPD report with data visualization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KlavierCat/pretty-jscpd.git"
  },
  "private": false,
  "keywords": [
    "copy-paste detection",
    "jscpd",
    "pmd",
    "data visualisation",
    "data visualization",
    "clean code"
  ],
  "author": "KlavierCat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KlavierCat/pretty-jscpd/issues"
  },
  "homepage": "https://github.com/KlavierCat/pretty-jscpd#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx pages __tests__ e2e; stylelint \"{styles,public,pages}/**/*.{css,scss}\"",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx pages __tests__ e2e; stylelint --fix \"{styles,public,pages}/**/*.{css,scss}\"",
    "eslint-check": "eslint-config-prettier pages/index.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "e2e": "npx codeceptjs run --steps",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test && npm run e2e"
    }
  },
  "lint-staged": {
    "*.{json,md,html}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.css": "stylelint --fix"
  },
  "dependencies": {
    "next": "10.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "codeceptjs": "^3.0.5",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
